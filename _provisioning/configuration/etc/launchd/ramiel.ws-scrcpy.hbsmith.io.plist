<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>Label</key>
        <string>ramiel.ws-scrcpy.hbsmith.io</string>

        <key>KeepAlive</key>
        <true/>
        <key>StartInterval</key>
        <integer>15</integer>

        <key>RunAtLoad</key>
        <true/>

        <key>WorkingDirectory</key>
        <string>/opt/ramiel/ws-scrcpy</string>
        <key>ProgramArguments</key>
        <array>
            <string>./_provisioning/01.run.sh</string>
        </array>

        <key>StandardOutPath</key>
        <string>/var/log/ramiel/ws-scrcpy.stdout.log</string>
        <key>StandardErrorPath</key>
        <string>/var/log/ramiel/ws-scrcpy.stderr.log</string>
    </dict>
</plist>